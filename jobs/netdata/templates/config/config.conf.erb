[global]
	debug flags = 0x10000000
	hostname = <%= spec.deployment %> <%= spec.job.name %>/<%= spec.index %>
	history = 172800
	access log = none
	bind socket to IP = 0.0.0.0
	disconnect idle web clients after seconds = 3600
	config directory = /var/vcap/jobs/netdata/config
	# plugins directory = /usr/libexec/netdata/plugins.d
	web files directory = /var/vcap/packages/netdata/share/netdata/web
	cache directory = /var/vcap/store/netdata/cache
	# lib directory = /var/lib/netdata
	log directory = /var/vcap/sys/log
	# host access prefix =
	home directory = /var/vcap/store/netdata
	# memory deduplication (ksm) = yes
	debug log = /var/vcap/sys/log/netdata/debug.log
	error log = /var/vcap/sys/log/netdata/error.log
	# errors flood protection period = 1200
	# errors to trigger flood protection = 200
	# memory mode = save
	# update every = 1
	# pthread stack size = 8388608
	run as user = root
	# web files owner = netdata
	# web files group = netdata
	# http port listen backlog = 100
	# port = 19999
	# ip version = any
	# multi threaded web server = yes
	# respect web browser do not track policy = no
	# enable web responses gzip compression = yes
	# web compression strategy = default
	# web compression level = 3
